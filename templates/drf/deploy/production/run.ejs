#!/usr/bin/env bash

cd /usr/share/webapps/<%= projectName %>/backend/<%= projectName %>

mkdir -p /usr/share/webapps/<%= projectName %>/backend/<%= projectName %>/var/run
rm -f /usr/share/webapps/<%= projectName %>/backend/<%= projectName %>/var/run/*

exec /usr/share/envs/<%= projectName %>/bin/gunicorn config.wsgi -c deploy/production/gunicorn.conf.py  --env DJANGO_SETTINGS_MODULE=config.settings.production